<%= form_for @gallery, :html => {:multipart => true} do |f| %>
  <%#= f.error_messages %>
   <%#= f.hidden_field :user_id %>
 
  <p>
    <%= f.label :name %><br />
    <%= f.text_field :name %>
  </p>
  <p>
    <%= f.label :des %><br />
    <%= f.text_field :des %>
  </p>
  <p>
    <%= f.label :avatar %><br />
    <%= f.file_field :avatar %>
  </p>
  
  <p><%= f.submit %></p>
<% end %>


